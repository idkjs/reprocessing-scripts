{
  "name": "reprocessing-scripts",
  "version": "1.3.6",
  "bin": {
    "rsb": "./rsb.sh"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "Jared Forsyth",
  "license": "MIT",
  "scripts": {
    "start": "bsb -make-world -backend native -w",
    "build": "bsb -clean-world -make-world -backend native",
    "clean": "bsb -clean-world",
    "reformat": "bsrefmt --in-place $(find src -name '*.re' -print) -w 80",
    "build-watch": "bsb -clean-world -make-world -backend native -- -w"
  },
  "dependencies": {
    "bsb-native": "5.0.3",
    "bs-platform-native": "0.0.12",
    "ios-sim": "9.0.0",
    "minimist.re": "^0.0.5",
    "pack.re": "0.2.0",
    "reason-cli-tools": "^1.0.4",
    "reason-simple-server": "^1.2.1",
    "rex-json": "1.4.0"
  }
}
